print(19225048)
age <- c("5-6 years", "7-8 years", "9-10 years")
A <- c(18, 2, 20)
B <- c(22, 28, 10)
C <- c(20, 40, 40)
df <- data.frame(A, B, C, row.names = age)
cov_BC <- cov(df$B, df$C)
print(cov_BC)
cov_matrix <- cov(df)
print(cov_matrix)
cor_BC <- cor(df$B, df$C)
print(cor_BC)
cor_matrix <- cor(df)
print(cor_matrix)
